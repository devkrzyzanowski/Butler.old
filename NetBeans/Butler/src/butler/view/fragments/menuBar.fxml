<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="anchorPane" maxHeight="25.0" minHeight="25.0" minWidth="200.0" prefHeight="25.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="butler.controller.fragments.MenuBarController">
   <children>
      <MenuBar fx:id="menuBar" layoutY="1.0" maxHeight="25.0" minHeight="25.0" prefHeight="25.0" prefWidth="200.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="1.0">
         <menus>
            <Menu mnemonicParsing="false" text="%menuBar.file">
               <items>
                  <MenuItem mnemonicParsing="false" text="Zamknij" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="%menuBar.settings">
               <items>
                  <MenuItem mnemonicParsing="false" text="Delete" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="%menuBar.help">
               <items>
                  <MenuItem mnemonicParsing="false" text="Sprawdź aktualizację" />
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <MenuItem mnemonicParsing="false" onAction="#openLicenseDialog" text="Zastosowane licencje" />
                  <MenuItem mnemonicParsing="false" text="Warunki umowy" />
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <MenuItem mnemonicParsing="false" text="Kontakt" />
                  <MenuItem mnemonicParsing="false" text="Pomoc" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
   </children>
</AnchorPane>
